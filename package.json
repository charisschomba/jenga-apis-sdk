{
  "name": "jenga-apis-sdk",
  "version": "1.0.4",
  "keywords": [
    "jenga",
    "apis",
    "sdk",
    "jenga-apis-sdk"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/charisschomba/jenga-apis-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/charisschomba/jenga-apis-sdk/issues"
  },
  "homepage": "https://charisschomba.github.io/jenga-apis-sdk/index.html",
  "author": "Chariss Chomba <charisschomba@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "output": {
    "format": [
      "cjs",
      "esm",
      "umd"
    ],
    "fileName": "index",
    "sourceMap": false,
    "globals": {
      "axios": "axios",
      "crypto-js": "crypto-js"
    },
    "exports": "named"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "browser": "./dist/index.umd.js"
  },
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && microbundle --tsconfig tsconfig.json --no-sourcemap",
    "dev": "microbundle watch",
    "prepublish": "npm-version"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "express": "^4.18.2",
    "microbundle": "^0.15.1",
    "nodemon": "^2.0.22",
    "npm-version": "^1.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.28",
    "typedoc-plugin-no-inherit": "^1.4.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "crypto-js": "^4.1.1"
  }
}
